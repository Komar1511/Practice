<Window x:Class="Practice.Capcha"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice"
        mc:Ignorable="d"
        Title="Capcha" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBox IsHitTestVisible="False" Width="85" Height="40" x:Name="txtkapcha" Text="Капча" FontFamily="Curlz MT" FontSize="20" Background="NavajoWhite"/>
        <Button Content="Генерация" Click="Genkapchaa"/>
        <TextBox x:Name="txtkek" TextWrapping="Wrap" Text="Введите" FontSize="20"/>
        <Button Content="Ввод" Click="Checkapcha"/>
    </StackPanel>
</Window>
